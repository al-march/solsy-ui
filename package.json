{
  "name": "solsy",
  "version": "0.0.1",
  "private": true,
  "description": "UI library with DaysiUi built with Solid.js.",
  "keywords": [
    "components",
    "solid",
    "solid-js",
    "solid.js",
    "solidjs",
    "ui",
    "ux"
  ],
  "homepage": "https://solsy.io",
  "bugs": {
    "url": "https://github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Alexander Marchenko",
      "email": "valverin95@gmail.com",
      "url": "https://github.com/DanzigWebb"
    }
  ],
  "workspaces": [
    "packages/ui",
    "packages/forms"
  ],
  "scripts": {
    "build": "pnpm build:ts",
    "build:ts": "tsc --build tsconfig.build.json",
    "check": "pnpm format:check && pnpm lint",
    "clean:ts": "tsc --build tsconfig.build.json --clean",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "lint": "eslint --ignore-path .gitignore .",
    "prepare": "husky install",
    "test": "jest",
    "watch": "tsc --build tsconfig.build.json -w"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@solidjs/testing-library": "^0.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-jest": "^29.5.0",
    "babel-preset-solid": "^1.7.4",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.12.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sort-json": "^1.0.0",
    "regenerator-runtime": "^0.13.11",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.7.6",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "info": "solsy is simple ui library with DaisyUI styles"
}
