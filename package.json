{
  "name": "solsy",
  "version": "0.0.1",
  "private": true,
  "description": "UI library with DaysiUi built with Solid.js.",
  "keywords": [
    "components",
    "solid",
    "solid-js",
    "solid.js",
    "solidjs",
    "ui",
    "ux"
  ],
  "homepage": "https://solsy.io",
  "bugs": {
    "url": "https://github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Alexander Marchenko",
      "email": "valverin95@gmail.com",
      "url": "https://github.com/DanzigWebb"
    }
  ],
  "workspaces": [
    "packages/ui",
    "packages/forms"
  ],
  "scripts": {
    "build": "pnpm build:ts",
    "build:ts": "tsc --build tsconfig.build.json",
    "check": "pnpm format:check && pnpm lint",
    "clean:ts": "tsc --build tsconfig.build.json --clean",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "lint": "eslint --ignore-path .gitignore .",
    "prepare": "husky install",
    "test": "jest",
    "watch": "tsc --build tsconfig.build.json -w"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^27.5.1",
    "babel-preset-solid": "^1.3.13",
    "eslint": "^8.12.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-solid": "^0.4.6",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.7",
    "prettier": "^2.6.2",
    "prettier-plugin-packagejson": "^2.2.17",
    "prettier-plugin-sort-json": "^0.0.2",
    "regenerator-runtime": "^0.13.9",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.3.13",
    "solid-testing-library": "^0.3.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "info": "solsy is simple ui library with DaisyUI styles"
}
